{
    "name": "smith981/silex-skeleton",
    "description": "This is a working skeleton used to rapidly develop a new Silex site, with Twitter Bootstrap, Twig, and a Doctrine ORM provider installed and registered. See https://github.com/smith981/silex-skeleton for installation instructions.",
    "authors": [
        {
            "name": "Ed Smith",
            "email": "smith981@gmail.com"
        }
    ],
    "repositories": [
    {
        "type": "composer",
        "url": "https://packages.zendframework.com/"
    }
    ],
    "require": {
        "php": ">=5.3",
        "silex/silex": "1.0.*@dev",
        "dflydev/doctrine-orm-service-provider": "1.0.*@dev",
        "twig/twig": "1.12.*",
        "zendframework/zend-code": "2.*",
        "smith981/form-generator": "dev-master"
    },
    "autoload": {
        "psr-0": { "": "entities/"}
    },
    "minimum-stability": "dev"
}